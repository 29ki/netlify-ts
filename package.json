{
  "name": "netlify-ts",
  "version": "0.5.0",
  "description": "Turn your Netlify CMS collections into TypeScript typings!",
  "repository": "jsaari97/netlify-ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Jim Saari",
  "keywords": [
    "netlify",
    "typescript",
    "types",
    "generate"
  ],
  "engines": {
    "node": ">=10.23.0"
  },
  "license": "MIT",
  "bin": "./bin/run",
  "files": [
    "/bin",
    "/lib"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --fix",
    "kitchen-sink": "./bin/run kitchen-sink.yml",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "js-yaml": "^3.14.1",
    "ora": "^5.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.5",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
