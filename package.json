{
  "name": "netlify-ts",
  "version": "0.3.0",
  "description": "Turn your Netlify CMS collections into TypeScript typings!",
  "repository": "jsaari97/netlify-ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Jim Saari",
  "keywords": [
    "netlify",
    "typescript",
    "types",
    "generate"
  ],
  "engines": {
    "node": ">=10.23.0"
  },
  "license": "MIT",
  "bin": "./bin/run",
  "files": [
    "/bin",
    "/lib"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint '*/**/*.{js,ts}' --fix --ignore-pattern lib/**/*",
    "kitchen-sink": "./bin/run kitchen-sink.yml",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "js-yaml": "^3.14.0",
    "ora": "^5.1.0",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.5",
    "@types/yargs": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
